{
	"name" : "gulpgenerator",
	"description" : "generator for gulpfiles",
	"authors" : [{
			"name" : "Elger van Boxtel",
			"email" : "elgervb@gmail.com",
			"homepage" : "http://elgervanboxtel.nl",
			"role" : "developer"
		}
	],
	"require" : {
		"elgervb/php-router" : "master-dev",
		"elgervb/php-http" : "master-dev",
		"elgervb/php-view" : "master-dev",
		"elgervb/php-handler" : "master-dev"
	},
	"require-dev" : {
		"phpunit/phpunit" : "^4.8"
	},
	"autoload" : {
		"psr-0" : {
			"gulp" : [
				"server/app",
				"server/tests"
			]
		},
		"classmap" : [
			"app/"
		]
	},
	"support" : {
		"issues" : "https://github.com/elgervb/gulpgenerator/issues",
		"email" : "elgervb@gmail.com"
	},
	"repositories" : [{
			"type" : "vcs",
			"url" : "https://github.com/elgervb/compact"
		}
	]
}